{
  "name": "tano-ui",
  "version": "0.0.2",
  "type": "module",
  "files": [
    "dist",
    "README.md",
    "package.json"
  ],
  "main": "./dist/tano-ui.umd.cjs",
  "types": "./dist/tano-ui.d.ts",
  "exports": {
    ".": {
      "import": "./dist/tano-ui.js",
      "require": "./dist/tano-ui.umd.cjs"
    },
    "./dist/tano-ui.css": "./dist/tano-ui.css"
  },
  "sideEffects": false,
  "scripts": {
